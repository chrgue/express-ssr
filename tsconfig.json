    "module": "esnext",
    "target": "esnext",
    "sourceMap": true,
    "noImplicitAny": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
